@model IEnumerable<PlayaInteligenteWeb.Models.ViewModel.DarsenaViewModel>

@{
    ViewBag.Title = "Home";
}

<p></p>
<div class="row">
    <div class="col-md-6">
            @Html.ActionLink("Estadias En Curso", "EstadiasEnCurso", null, new { @class = "btn btn-primary btn-lg" })
    </div>
    <div class="col-md-6">
        @Html.ActionLink("Estadias Completas", "EstadiasCompletas", null, new { @class = "btn btn-primary btn-lg" })
    </div>
</div>

<h2>Estado actual de Darsenas</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Numero)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Numero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
        </tr>
    }
</table>

